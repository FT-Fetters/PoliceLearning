<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lyun.policelearning.dao.paper.SchedulePaperDao">
    <insert id="insert" useGeneratedKeys="true" keyProperty="id" parameterType="SchedulePaper">
        insert into schedule_paper(cron,j,s,m,title) values(#{cron},#{j},#{s},#{m},#{title})
    </insert>
</mapper>
